<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="VkShopManager" namespace="VkShopManager.Domain">
  <class name="Album" lazy="false" table="albums"> 
    <id name="Id" column="id">
      <generator class="native"/>
    </id>
    <property name="VkId" column="vk_id" type="Int64" not-null="true"/>
    <property name="Title" column="title" type="String" not-null="true"/>
    <property name="CreationDate" column="creation_date" type="DateTime" not-null="true"/>
    <property name="ThumbImg" column="thumb_img" type="String"/>
  </class>
</hibernate-mapping>