        <ListBox HorizontalAlignment="Left" Margin="12,12,0,49" Name="lbDeliveries" Width="265" SelectionChanged="lbDeliveries_SelectionChanged" MouseDoubleClick="lbDeliveries_MouseDoubleClick" />
